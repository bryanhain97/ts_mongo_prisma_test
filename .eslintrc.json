{
  "extends": "next/core-web-vitals",
  "rules": {
    "prefer-const": "error",
    "no-duplicate-imports": "error",
    "no-undef": "error",
    "no-var": "error",
    "camelcase": "warn",
    "no-trailing-spaces": "error",
    "quotes": [
      "warn",
      "single"
    ],
    "arrow-parens": [
      "error",
      "always"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}